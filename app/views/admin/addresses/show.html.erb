<table>
  <thead>
    <tr>
      <th style="width:30%">属性</th>
      <th>值</th>
    </tr>
  </thead>

  <tbody>

    <tr>
      <th><%= Address.human_attribute_name(:cid) %>:</th>
      <td><%= @address.cid %></td>
    </tr>

    <tr>
      <th><%= Address.human_attribute_name(:parent_city) %>:</th>
      <td><%= @address.parent_city %></td>
    </tr>

    <tr>
      <th><%= Address.human_attribute_name(:lon) %>:</th>
      <td><%= @address.lon %></td>
    </tr>

    <tr>
      <th><%= Address.human_attribute_name(:lat) %>:</th>
      <td><%= @address.lat %></td>
    </tr>

  </tbody>
</table>
<div class="actions">
  <%= link_to t('actions.edit'), edit_admin_address_path(@address), class: "ui button primary" %>
  <%= link_to t('buttons.back'), admin_addresses_path, class: "ui button" %>
</div>

